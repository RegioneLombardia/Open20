{
    "name": "open20/taskmanager",
    "description": "Sistema AMOS per la gestione dei task",
    "keywords": ["amos", "yii2", "task-manager"],
    "homepage": "http://git.open20.it/open20/task-manager",
    "type": "component",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "http://git.open20.it/open20/task-manager/issues",
        "forum": "http://www.example.com/forum/",
        "wiki": "http://git.open20.it/open20/task-manager/wikis/home",
        "source": "http://git.open20.it/open20/task-manager/tree/master"
    },
    "require": {
        "php": ">=5.4.0",
        "open20/amos-admin" : "*",
        "open20/amos-attachments" : "*",
        "open20/amos-core" : "*",
        "open20/amos-layout" : "*"
    },
    "config": {
        "process-timeout": 1800,
        "secure-http": false
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload": {
        "psr-4": {
            "open20\\amos\\taskmanager\\": "src"
        }
    },
    "require-dev": {
        "yiisoft/yii2-faker": "*"
    }
}
