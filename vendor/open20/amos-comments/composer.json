{
    "name": "open20/amos-comments",
    "description": "Comments for contents - plugin",
    "keywords": ["amos", "yii2", "comments"],
    "type": "plugin",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.4.0",
        "open20/amos-core" : "*",
