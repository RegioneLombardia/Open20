{
  "name": "open20/amos-collaborations",
  "description": "AMOS Collaboration Opportunities",
  "keywords": ["amos", "yii2", "collaborations"],
  "homepage": "http://www.open20.it/",
  "type": "component",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "http://www.example.com/",
    "forum": "http://www.example.com/forum/",
    "wiki": "http://www.example.com/wiki/",
    "source": "https://github.com/open20/amos-collaborations"
  },
  "require": {
    "php": ">=5.4.0",
    "open20/amos-notify" : "*",
    "open20/amos-core" : "*",
    "open20/amos-admin" : "*",
    "open20/amos-tag" : "*",
    "open20/amos-attachments" : "*",
    "amos/site-management": "^1.1.0"
  },
  "config": {
    "process-timeout": 1800,
    "secure-http": false
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "autoload": {
    "psr-4": {
      "open20\\amos\\collaborations\\": "src"
    }
  },
  "require-dev": {
    "yiisoft/yii2-faker": "*"
  }
}
