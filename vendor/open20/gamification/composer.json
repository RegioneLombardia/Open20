{
    "name": "open20/gamification",
    "description": "Sistema Open2 per gamification",
    "keywords": ["open2", "yii2", "gamification"],
    "homepage": "http://www.open20.it/",
    "type": "component",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "http://www.example.com/",
        "forum": "http://www.example.com/forum/",
        "wiki": "http://www.example.com/wiki/",
        "source": "https://github.com/open20/gamification"
    },
    "require": {
        "php": ">=7.0.0"
    },
    "config": {
        "process-timeout": 1800,
        "secure-http": false
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload": {
        "psr-4": {
            "open20\\gamification\\": "src"
        }
    },
    "require-dev": {
        "yiisoft/yii2-faker": "*"
    }
}
